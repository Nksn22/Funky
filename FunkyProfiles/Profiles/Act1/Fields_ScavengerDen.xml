          <PriorityScene sceneName="CaveEntrance" />
        </PriorityScenes>
        <IgnoreScenes>
          <IgnoreScene sceneName="Exit" />
        </IgnoreScenes>
      </TrinityExploreDungeon>

      <MoveToActor questId="312429" stepId="2" actorId="175501" interactRange="8" isPortal="True" destinationWorldId="81163" />
    </If>

    <If condition="CurrentLevelAreaId == 81165">
      <WaitTimer questId="312429" stepId="2" waitTime="500" />
      <TrinityExploreDungeon questId="1" stepId="2" boxTolerance="0.18" boxSize="20" until="ExitFound" exitNameHash="925091455" ignoreMarkers="False">
