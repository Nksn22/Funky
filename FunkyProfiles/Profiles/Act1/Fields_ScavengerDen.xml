    <If condition="CurrentLevelAreaId == 19952">
      <TrinityExploreDungeon questId="312429" stepId="2" until="ObjectFound" exitNameHash="0" actorId="175501" pathPrecision="25" boxSize="15" boxTolerance="0.55" objectDistance="35">
